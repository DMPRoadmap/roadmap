{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "../Documentation/Implementation/data_model/JSON/",
    "title": "Reuse data",
    "type": "object",
    "properties": {
        "justification": {
            "label@fr_FR": "Justification",
            "label@en_GB": "Justification",
            "type": "string"
        },
        "reusedData":{
            "label@fr_FR":"Produit de recherche réutilisé",
            "label@en_GB": "Reused research output",
            "type": "array",
            "items": {
                "type": "object",
                "classname": "reused_data",
                "properties": {
                    "dbid": {
                        "type": "number"
                    }
                },
                "required": ["dbid"]
            }
        },
        "cost": {
            "label@fr_FR": "Coût d'acquisition",
            "label@en_GB": "Acquisition cost",
            "type": "array",
            "items": {
                "type": "object",
                "classname": "cost",
                "properties": {
                    "dbid": {
                        "type": "number"
                    }
                },
                "required": [
                    "dbid"
                ]
            }
        }
    },
         
    "required":["justification"]
    
    }