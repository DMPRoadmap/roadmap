{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "../Documentation/Implementation/data_model/JSON/",
    "title": "Legal Issues",
    "description": "The documentation of legal issues schema",
    "type": "object",
    "properties": {
        "legalItem": {
            "label": "Legal item",
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "classname": "legal_item",
                "properties": {
                    "dbid": {
                        "type": "number"
                    }
                },
                "required": ["dbid"]
            }
        },
        "staffMember": {
            "label": "Staff members",
            "type": "array",
            "items": {
                "type": "object",
                "classname": "staff_member",
                "properties": {
                    "dbid": {
                        "type": "number"
                    }
                },
                "required": ["dbid"]
            }
        }
    },
    "requested": [
        "legalItem"
    ]
}