{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "../Documentation/Implementation/data_model/Json/",
    "title": "DataReuse",
    "description": "DataReuse template",
    "type": "object",
    "extensible": 1,
    "properties": {
        "justification": {
            "type": "string",
            "description": "Justification de la réutilisation ou non réutilisation de données existantes",
            "label@fr_FR": "Justification",
            "label@en_GB": "Justification"
        },
        "reusedData": {
            "type": "array",
            "items": {
                "type": "object",
                "class": "reused_data",
                "extensible": "1",
                "properties": {
                    "dbid": {
                        "type": "number"
                    }
                },
                "schema_id": "29",
                "required": [
                    "dbid"
                ]
            },
            "description": "Jeu de données réutilisé",
            "label@fr_FR": "Produit de recherche réutilisé",
            "label@en_GB": "Reused research output"
        },
        "cost": {
            "type": "array",
            "items": {
                "type": "object",
                "class": "cost",
                "extensible": "1",
                "properties": {
                    "dbid": {
                        "type": "number"
                    }
                },
                "schema_id": "5",
                "required": [
                    "dbid"
                ]
            },
            "description": "Coûts éventuels liés à l'achat de données",
            "label@fr_FR": "Coût d'acquisition",
            "label@en_GB": "Acquisition cost"
        }
    },
    "required": [
        "justification"
    ],
    "to_string": [
        "DataReuse: to_string not defined"
    ]
}