{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "../Documentation/Implementation/data_model/Json/",
    "title": "Budget",
    "description": "Budget template",
    "type": "object",
    "properties": {
        "description": {
            "type": "string",
            "description": "Description générale des coûts et des ressources (rôles, responsabilités, …) utilisés pour la gestion des données de la recherche"
        },
        "budgetItem": {
            "type": "array",
            "items": {
                "type": "object",
                "class": "budget_item",
                "properties": {
                    "dbid": {
                        "type": "number"
                    }
                },
                "schema_id": "32",
                "required": [
                    "dbid"
                ]
            },
            "description": "Elément de description des coûts"
        }
    },
    "required": [
        "description"
    ],
    "to_string": [
        "Budget: to_string not defined"
    ]
}