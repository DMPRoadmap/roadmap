#: ../../app/views/static_pages/about_us.html.erb:22
msgid ""
"get involved<em class=\"sr-only\"> (new window)</em><span class=\"new-window-popu"
"p-info\">%{open_in_new_window_text}</span>"
msgstr ""
"<span class=\"new-window-popup-info\">mach</span> mit <em class=\"sr-only\">(neues"
