msgid ""
"<p><span style=\"font-weight: 400;\">Des informations de base se trouvent dans l"
"e </span><a href=\"http://guides.bib.umontreal.ca/disciplines/500-Gestion-des-d"
"onnees-de-recherche?tab=2301\" target=\"_blank\"><span style=\"font-weight: 400;\">"
"guide sur la GDR</span></a><span style=\"font-weight: 400;\">.</span></p>"
msgstr ""

#: ../../tmp/translation/database_strings.rb:776
msgid ""
"<p><span style=\"font-weight: 400;\">Concernant les</span><strong> cahiers de la"
"boratoires </strong><span style=\"font-weight: 400;\">dans certaines disciplines"
", des informations se trouvent dans le </span><a href=\"http://guides.bib.umont"
"real.ca/disciplines/500-Gestion-des-donnees-de-recherche?tab=2301\" target=\"_bl"
"ank\"><span style=\"font-weight: 400;\">guide sur la GDR</span></a><span style=\"f"
"ont-weight: 400;\">.</span></p>"
msgstr ""

#: ../../tmp/translation/database_strings.rb:777
