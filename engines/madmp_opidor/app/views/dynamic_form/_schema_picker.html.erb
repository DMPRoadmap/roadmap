<%# locals: { madmp_schema, pickable_schemas, question, research_output } %>
<strong><%= _("You can pick a different question form") %></strong>

<%= select_tag "schema_picker",
      options_for_select(
        pickable_schemas.map {|v| [
          v.label, 
          v.id
        ] 
      },
      selected: madmp_schema.id),
      "data-toggle": "tooltip",
      "data-fragment-id": fragment&.id || "",
      class: "form-control schema_picker"%>