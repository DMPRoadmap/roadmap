<% notifications.each do |a| %>
  <% unless a.acknowledged? %>
    <div class="alert alert-<%= a.level %>">
      <span class="fa <%= fa_classes(a) %>"></span>
      <span class="aria-only"><strong><%= "#{a.level.capitalize}:" %></strong></span>
      <span><%= raw a.body %></span>
      <% if a.dismissable? %>
        <button class="close" data-dismiss="alert" data-url="<%= super_admin_notification_acknowledge_url(a) %>" data-remote="true" data-method="post" aria-label="Close">
          <span class="fa fa-times-circle" aria-hidden="true"></span>
        </button>
      <% end %>
    </div>
  <% end %>
<% end %>
