<h1>Edit Domain</h1>

<%= form_with model: @org_domain, url: org_domain_path(@org_domain), method: :patch, local: true do |form| %>
  <div>
    <%= form.label :domain %><br>
    <%= form.text_field :domain %>
  </div>

  <br>
  <%= form.submit "Update Domain" %>
<% end %>

<%= link_to "Back", org_domain_show_path %>
